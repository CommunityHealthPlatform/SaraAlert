  <% content_for :assets do %>
    <%= javascript_packs_with_chunks_tag 'moment' %>
  <% end %>
  
  
  
  <%= react_component('public_health/Workflow', {
                        authenticity_token: form_authenticity_token,
                        abilities: {
                          analytics: current_user.can_view_analytics?,
                          enrollment: current_user.can_create_patient?
                        },
                        jurisdiction: {
                          id: current_user.jurisdiction_id,
                          path: current_user.jurisdiction[:path]
                        },
                        tabs: {                         
                          enrolled: {
                            label: 'Enrolled',
                            variant: 'success',
                            tooltip: 'enrolled',
                            description: 'Monitorees currently enrolled in the system.'
                          },
                          closed: {
                            label: 'Closed',
                            variant: 'secondary',
                            tooltip: 'closed',
                            description: 'Monitorees not currently being monitored.'
                          },
                          all: {
                            label: 'All Monitorees',
                            variant: 'primary',
                            description: 'All Monitorees in this trial.'
                          }
                        },
                        default_tab: 'enrolled',
                        jurisdiction_paths: @possible_jurisdiction_paths,
                        all_assigned_users: @all_assigned_users
                      }) %>
